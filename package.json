{
  "name": "portfolio-2020",
  "version": "1.0.0",
  "description": "A redesign  of my portfolio site",
  "main": "index.js",
  "scripts": {
    "dev:content": "npx @11ty/eleventy --watch --serve",
    "dev:sass-init": "node-sass src/assets/styles -o src/assets/styles/compiled",
    "dev:sass-watch": "node-sass src/assets/styles -o src/assets/styles/compiled -w",
